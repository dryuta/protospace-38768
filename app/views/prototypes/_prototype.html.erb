<div class="card">
  <%= image_tag prototype.image, class: 'prototype-image' %>
  <div class="card__body">
    <%= link_to prototype.title, root_path, class: :card__title%>
    <p class="card__summary">
      <%= prototype.concept %>
    </p>
    <%= link_to "by" + prototype.user.name, root_path, class: :card__user %>
  </div>
</div>